<?xml version="1.0" encoding="UTF-8"?>
<extentreports>
    <configuration>
        <!-- report theme -->
        <!-- standard, dark -->
        <theme>dark</theme>

        <!-- document encoding -->
        <!-- defaults to UTF-8 -->
        <encoding>UTF-8</encoding>

        <!-- protocol for script and stylesheets -->
        <!-- defaults to https -->
        <protocol>http</protocol>

        <!-- title of the document -->
        <documentTitle>Extent Framework</documentTitle>

        <!-- report name - displayed at top-nav -->
        <reportName>Abhishek Sen</reportName>

        <!-- timestamp format -->
        <timeStampFormat>MMM dd, yyyy HH:mm:ss</timeStampFormat>
        <!-- accept de thumbnail -->
        <thumbnailForBase64>true</thumbnailForBase64>
        <!-- custom javascript -->
        <script>
            <![CDATA[
                $(document).ready(function() {
                    $('#parentWithoutNodes').prop("checked", true).click();
                    $('#test-count-setting > div.modal-footer > a')[0].click();
                });
            ]]>
        </script>

        <!-- custom styles -->
        <styles>
            <![CDATA[
                .step-name {
                    opacity:45;
                }
            ]]>
        </styles>
    </configuration>
</extentreports>